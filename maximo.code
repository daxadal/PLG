method maximo
takes input array of 10 int a;
declares output int max = 0;
makes
int i = 0;
max = a @ 0;
while i < 10 do
	if max < a @ i then
		max = a @ i;
	done;
	i = i + 1;
done;
returns output;

main start
array of 7 int a = {5,4,8,10,0,3,9};
	int max = 0;
	call maximo with a receiving max;
end;
